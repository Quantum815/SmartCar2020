int CheckCRC(void)
{
	unsigned short tmp =0xffff;
    for(int n = 0; n < 6; n++){
        tmp = VL53L1RX_Data[n] ^ tmp;
        for(int i = 0;i < 8;i++){ 
            if(tmp & 0x01){
                tmp = tmp >> 1;
                tmp = tmp ^ 0xa001;
            }   
            else{
                tmp = tmp >> 1;
            }   
        }   
    }  
}